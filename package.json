{
  "name": "cypress-search-feature",
  "version": "1.0.0",
   "author": "shruti",
  "description": "This project contains Cypress tests to validate the search feature across desktop and mobile views",
  "scripts": {
    "cypress-desktop:ui": "npx cypress open --env viewportType=desktop",
    "cypress-mobile:ui": "npx cypress open --env viewportType=mobile",
    "cypress-desktop:ci": "npx cypress run --env viewportType=desktop",
    "cypress-mobile:ci": "npx cypress run --env viewportType=mobile"
  },
  "devDependencies": {
    "cypress": "^13.15.2",
    "eslint": "^9.14.0"
  }
}
